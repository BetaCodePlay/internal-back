{
    "name": "dotworkers/dotpanel",
    "type": "project",
    "description": "Dotpanel project",
    "keywords": [
        "dotpanel",
        "project"
    ],
    "license": "Proprietary",
    "authors": [
        {
            "name": "Damelys Espinoza",
            "email": "damelys.espinoza@dotworkers.com"
